{
  "name": "checkpoint-client",
  "description": "javascript checkpoint client",
  "version": "1.1.33",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "!dist/examples"
  ],
  "scripts": {
    "tsc": "tsc",
    "watch": "rollup -c -w",
