    "test-only": "jest --testTimeout 10000",
    "prepublish": "npm run lint && npm run build && npm run test",
    "lint": "prettier src/**/* -c",
    "format": "prettier --write src/**/*"
  },
  "license": "MIT",
  "dependencies": {
    "ci-info": "4.0.0",
    "env-paths": "2.2.1",
