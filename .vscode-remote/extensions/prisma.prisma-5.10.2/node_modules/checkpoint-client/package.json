    "uuid": "9.0.1"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "21.1.0",
    "@rollup/plugin-json": "6.0.1",
    "@rollup/plugin-node-resolve": "9.0.0",
    "@rollup/plugin-sucrase": "5.0.2",
    "@types/concat-stream": "2.0.3",
    "@types/is-ci": "3.0.4",
    "@types/jest": "29.5.10",
    "@types/ms": "0.7.34",
    "@types/node": "16.18.66",
    "@types/node-fetch": "2.6.9",
    "@types/uuid": "9.0.7",
    "boxen": "4.2.0",
    "concat-stream": "2.0.0",
    "globby": "11.1.0",
    "is-ci": "3.0.1",
    "jest": "29.7.0",
    "prettier": "2.8.8",
    "rollup": "2.79.1",
    "tempfile": "3.0.0",
    "testaway": "2.0.1",
    "ts-node": "10.9.1",
    "typescript": "5.3.2"
